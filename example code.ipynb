#found 9465 cells containing 0.0
#replace 0.0 from corresponding values of ' MV ' column
def replace_zeros(x, y):
    if x == 0:
        return y
    else: 
        return x
df.columns
df['grand_total'] = df.apply(lambda z: replace_zeros(z['grand_total'], z[' MV ']), axis = 1)
